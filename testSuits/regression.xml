<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >



<suite name="Regression" verbose="1">
<listeners>
      <listener class-name = "com.qatools.report.CustomReporter" />
 </listeners>
	<test name="Firefox">
		<parameter name="browser" value="firefox"/>
		<packages>
			<package name="com.qatools.tests.*"/>
		</packages>
	</test>
	<test name="Chrome">
		<parameter name="browser" value="chrome"/>
		<packages>
			<package name="com.qatools.tests.*"/>
		</packages>
	</test>
</suite>